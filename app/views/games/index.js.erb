<% if session[:games] == 'all'%>
  $('#content').append("<%=j render 'all_list', games: @games, head: false, format: 'html' %>"); 
<%else%>
  $('#content').append("<%=j render 'last_list', games: @games, head: false, format: 'html' %>");
<%end%>
<% if @games.current_page == @games.total_pages %>
  $('#view-more').remove();
<% else %>
  $('#view-more a').attr('href', '<%= games_path(page: @games.current_page + 1) %>');
<% end %>
