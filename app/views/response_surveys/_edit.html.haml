.name_response[survey_response]
  %b= survey_response.name
  = link_to edit_survey_response_survey_path(survey_response.survey, survey_response), :method => :post, remote: true do
    %i.fa.fa-pencil

  = link_to survey_response_survey_path(survey_response.survey, survey_response), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, remote: true do
    %i.fa.fa-trash

